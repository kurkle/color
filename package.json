{
  "name": "@kurkle/color",
  "type": "module",
  "version": "0.4.0",
  "description": "css color parsing, manupulation and conversion",
  "sideEffects": false,
  "main": "dist/color.cjs",
  "module": "dist/color.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": {
      "import": "./dist/index.esm.d.ts",
      "require": "./dist/index.d.ts"
    },
    "import": "./dist/color.esm.js",
    "require": "./dist/color.cjs"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "eslint src/**/*.ts test/*.js scripts/*.js",
    "test": "node test/index.js",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc && touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurkle/color.git"
  },
  "files": [
    "dist/*",
    "dist/color.d.ts"
  ],
  "keywords": [
    "color",
    "colour",
    "css",
    "hsl",
    "hex",
    "rgb",
    "rgba",
    "hwb",
    "hsv",
    "cmyk"
  ],
  "author": "Jukka Kurkela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurkle/color/issues"
  },
  "homepage": "https://github.com/kurkle/color#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "assert": "^2.0.0",
    "benchmark": "^2.1.4",
    "chartjs-color": "^2.4.1",
    "chartjs-color-string": "^0.6.0",
    "child_process": "^1.0.2",
    "chroma-js": "^3.1.1",
    "color-name": "^2.0.0",
    "color-names": "^2.0.0",
    "color-parse": "^2.0.2",
    "color-parser": "^0.1.0",
    "color-string": "^2.0.1",
    "csscolorparser": "^1.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^9.15.0",
    "eslint-config-chartjs": "^0.3.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-typescript": "^0.14.0",
    "fs": "0.0.1-security",
    "perf_hooks": "0.0.1",
    "tinycolor2": "^1.4.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.6.2",
    "util": "^0.12.3"
  }
}
